package com.altimetrik.food.bean;

import java.util.HashMap;
import java.util.List;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;



@JsonInclude(Include.NON_NULL)
public class Reciepe {
	private HashMap<String, String> recipe;
	private String q;
	private List<MealInformation>hits;
	

	public String getQ() {
		return q;
	}

	public void setQ(String q) {
		this.q = q;
	}

	public HashMap<String, String> getRecipe() {
		return recipe;
	}

	public void setRecipe(HashMap<String, String> recipe) {
		this.recipe = recipe;
	}

	@Override
	public String toString() {
		return "Reciepe [recipe=" + recipe + ", q=" + q + "]";
	}

	

	
}
